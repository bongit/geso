<div class="col-lg-12">
	<div class="page-header">
	  <h1>素材のアップロード（確認）</h1></br>
	</div>
	<div class="well">
		<h3>ファイル内容</h3>
		<% if @game_asset != nil %>
		<ur>
			<% @game_asset.zip_includes.each do |fl| %>
			<li><%= fl %></li>
			<% end %>
		</ur>
		<% end %>
	</div>
	<div class="btn btn-default">
		<%= link_to "戻る", asset_file_upload_game_asset_path %>
	</div>

	<%= form_tag('asset_file_confirm') do |f| %>
    <%= hidden_field_tag :file_uploaded, :value => "true" %>
    <%= submit_tag "このファイルで登録", :class => 'btn btn-warning', :onclick => "return confirm('ファイルは後から変更できません。登録してよろしいですか？')" %>
    <% end %>
</div>

